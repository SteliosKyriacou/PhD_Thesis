reset
set term post eps m enhanced
set output 'Optimization.eps'
set size 0.8,0.8
set pointsize 0.2 
set grid
set title ''
set xr [0:1]
#set yr [0:10]
set xtics 1
set ytics 5000
set xlabel 'Year'
set ylabel ''
set yr [15000:65000]
set xr [2001:2012]
set title 'Publications regarding optimization'
plot 'pubs_optim' using 1:2 with impulse lw 40 t'http://www.sciencedirect.com'


